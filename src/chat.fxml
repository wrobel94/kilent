<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="580.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ChatController">
   <children>
      <TextArea fx:id="chatTextArea" editable="false" layoutX="14.0" layoutY="14.0" prefHeight="285.0" prefWidth="383.0" wrapText="true" />
      <TextArea fx:id="myMessageTextArea" layoutX="14.0" layoutY="312.0" prefHeight="74.0" prefWidth="383.0" wrapText="true" />
      <Button fx:id="sendPublishButton" layoutX="413.0" layoutY="312.0" mnemonicParsing="false" onAction="#onSendPublish" prefHeight="74.0" prefWidth="158.0" text="Send/Publish" />
      <ComboBox fx:id="roomOrContactComboBox" layoutX="413.0" layoutY="14.0" onAction="#comboBoxValue" prefHeight="25.0" prefWidth="158.0" />
      <ComboBox fx:id="roomsContactsComboBox" disable="true" layoutX="413.0" layoutY="59.0" prefHeight="25.0" prefWidth="158.0" />
      <Button fx:id="refreshButton" disable="true" layoutX="527.0" layoutY="99.0" mnemonicParsing="false" onAction="#refreshList" prefHeight="25.0" prefWidth="39.0" text="Ref" />
   </children>
</AnchorPane>
